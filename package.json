{
  "name": "bootstrap-custom-hugo",
  "version": "1.0.0",
  "description": "Project template for creating custom bootstrap using  gulp-rollup-bootstrap-custom and hugo",
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "bootstrap": "^4.5.0",
    "del": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-rollup-bootstrap-custom": "^1.0.0",
    "jquery": "^3.5.1",
    "merge-stream": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.16.1"
  },
  "main": "gulpfile.js",
  "scripts": {
    "build:gulp": "gulp build",
    "build:hugo": "hugo",
    "build": "run-s build:gulp build:hugo",
    "clean": "gulp clean",
    "watch:gulp": "gulp watch",
    "watch:hugo": "hugo server -D --disableFastRender",
    "watch": "run-p watch:hugo watch:gulp",
    "dev": "run-s build:gulp watch"
